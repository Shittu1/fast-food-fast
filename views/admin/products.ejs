<% include partials/header %>
<% include partials/admintitle %>
	

	<!-----------Admin Area---------->
	<section class="admindashboard generalfeatures">

		<% include partials/adminnavbar %>

		<div class="adminbody">
			<table id="allproducts">
				<thead>
					<tr>
						<th></th>
						<th>Product</th>
						<th>Price</th>
						<th>Date</th>
						<th>Edit</th>
						<th>Delete</th>
					</tr>
				</thead>

		<% delicacies.forEach((delicacy) => { %>		

				<tbody>
					<tr>
						<td><input type="checkbox"></td>
						<td><%= delicacy["name"] %></td>
						<td><%= delicacy["price"] %></td>
						<td><%= delicacy["date"] %></td>
						<td><a href=""><i class="far fa-edit"></i></a></td>
						<td><a href=""><i class="far fa-trash-alt"></i></a></td>
					</tr>
				</tbody>
		<% }); %>

				
			</table>
		</div>
		<br>
	</section>

	<br/>
